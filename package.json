{
  "name": "react-web-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check-watch": "npm run type-check -- --watch",
    "build-lib": "npm run build-types && npm run build-js",
    "build-types": "tsc --emitDeclarationOnly",
    "build-js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:config:dev": "tsc -p webpack/tsconfig-webpack.dev.json",
    "build:config": "tsc -p webpack/tsconfig-webpack.json",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --hot --open",
    "start:dev:board": "cross-env NODE_ENV=development BOARD=true webpack-dashboard -- webpack-dev-server --config webpack.config.dev.js --hot --open",
    "bundle": "cross-env NODE_ENV=production webpack",
    "analyse": "cross-env NODE_ENV=analyse webpack && npm run analyse-bundle",
    "analyse-bundle": "bundle-buddy www/dist/*.map",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "core-js": "2.6.3",
    "intl": "1.2.5",
    "es6-promise": "4.2.5",
    "url-polyfill": "1.1.3",
    "whatwg-fetch": "3.0.0",
    "ky": "0.7.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "7.3.1",
    "@types/jest": "23.3.13",
    "@types/react": "16.8.1",
    "@types/react-dom": "16.0.11",
    "@types/react-test-renderer": "16.0.3",
    "@types/webpack": "4.4.24",
    "babel-loader": "8.0.5",
    "babel-plugin-webpack-alias": "2.1.2",
    "bundle-buddy": "0.2.1",
    "clean-webpack-plugin": "1.0.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "cssnano": "4.1.8",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.6.5",
    "react-test-renderer": "16.7.0",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.1",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-dashboard": "2.1.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "keywords": [],

{
  "name": "react-web-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check-watch": "npm run type-check -- --watch",
    "build-lib": "npm run build-types && npm run build-js",
    "build-types": "tsc --emitDeclarationOnly",
    "build-js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:config:dev": "tsc -p webpack/tsconfig-webpack.dev.json",
    "build:config": "tsc -p webpack/tsconfig-webpack.json",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --hot --open",
    "bundle": "cross-env NODE_ENV=production webpack",
    "analyse": "cross-env NODE_ENV=analyse webpack && npm run analyse-bundle",
    "analyse-bundle": "bundle-buddy www/dist/*.map",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-loadable": "5.5.0",
    "core-js": "2.5.7",
    "intl": "1.2.5",
    "es6-promise": "4.2.5",
    "url-polyfill": "1.1.0",
    "whatwg-fetch": "3.0.0",
    "ky": "0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@types/react": "16.4.13",
    "@types/react-dom": "16.0.7",
    "@types/react-loadable": "5.4.1",
    "@types/webpack": "4.4.11",
    "babel-loader": "8.0.2",
    "babel-plugin-webpack-alias": "2.1.2",
    "bundle-buddy": "0.2.1",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "4.1.3",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.3",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.3.7",
    "style-loader": "0.23.0",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8"
  },
